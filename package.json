    "sanity": "^3.64.1",
    "sanity-plugin-markdown": "^5.0.0",
    "server-only": "^0.0.1",
    "slugify": "^1.6.6",
    "styled-components": "^6.1.13",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^20.17.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.3",
    "postcss": "^8.4.47",
    "require-in-the-middle": "^7.4.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3"
