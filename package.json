{
  "name": "next15-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "predev": "pnpm run typegen",
    "prebuild": "pnpm run typegen",
